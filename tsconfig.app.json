{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "types/**/*",
    "typing/**/*",
    "typings/**/*"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.src.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
